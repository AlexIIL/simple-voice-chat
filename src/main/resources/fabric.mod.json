{
  "schemaVersion": 1,
  "id": "voicechat",
  "version": "${version}",
  "name": "Simple Voice Chat",
  "description": "A working voice chat in Minecraft!",
  "authors": [
    "Max Henkel"
  ],
  "contact": {
    "website": "https://www.curseforge.com/minecraft/mc-mods/simple-voice-chat",
    "issues": "https://github.com/henkelmax/simple-voice-chat/issues",
    "sources": "https://github.com/henkelmax/simple-voice-chat"
  },
  "license": "All Rights Reserved",
  "icon": "assets/voicechat/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "de.maxhenkel.voicechat.VoicechatClient"
    ],
    "main": [
      "de.maxhenkel.voicechat.Voicechat"
    ]
  },
  "mixins": [
    "voicechat.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-command-api-v1": ">=${fabric_version}",
    "fabric-lifecycle-events-v1": ">=${fabric_version}",
    "fabric-networking-api-v1": ">=${fabric_version}",
    "fabric-key-binding-api-v1": ">=${fabric_version}",
    "fabric-rendering-v1": ">=${fabric_version}",
    "fabric-resource-loader-v0": ">=${fabric_version}",
    "minecraft": "${minecraft_dependency}",
    "java": ">=16"
  },
  "custom": {
    "voicechat": {
      "compatibilityVersion": ${compatibility_version}
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/4dH2zwTmyX"
      }
    }
  }
}
